{
  "name": "@activewidgets/angular",
  "version": "3.4.0",
  "description": "Datagrid component for angular",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "type": "module",
  "main": "./dist/ax-angular.umd.js",
  "module": "./dist/ax-angular.esm.js",
  "types": "./index.d.ts",
  "exports": {
    "import": "./dist/ax-angular.esm.js",
    "require": "./dist/ax-angular.umd.js"
  },
  "files": [
    "dist",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:meta && npm run build:js",
    "build:js": "rimraf --glob dist/* && rollup -c",
    "build:meta": "node types.config.js",
    "examples": "npm run build && vite build",
    "prepack": "npm run build && rimraf --glob out/*",
    "preview": "npm run examples && vite preview --open /",
    "start": "vite --open /",
    "test": "npm run examples && vitest run"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.4.0",
    "@activewidgets/frameworks": "3.4.0"
  },
  "peerDependencies": {
    "@angular/core": "*"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.16",
    "@activewidgets/options": "3.4.0",
    "@activewidgets/puppeteer": "0.1.2",
    "@angular/common": "^14",
    "@angular/compiler": "^14",
    "@angular/core": "^14",
    "@angular/platform-browser": "^14",
    "@angular/platform-browser-dynamic": "^14",
    "@rollup/plugin-node-resolve": "^15",
    "@rollup/plugin-terser": "^0.4",
    "@testing-library/dom": "^9",
    "ng-dynamic-component": "^10",
    "rimraf": "^4",
    "rollup": "^3",
    "rollup-plugin-md": "^1",
    "rollup-plugin-postcss": "^4",
    "rxjs": "~7.5.0",
    "vite": "^4",
    "zone.js": "~0.11.4"
  },
  "overrides": {
    "jest": "npm:style-inject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "activewidgets",
    "datagrid",
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://activewidgets.com"
}
