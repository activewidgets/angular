{
  "name": "@activewidgets/angular",
  "version": "3.1.2",
  "description": "Datagrid component for angular",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/ax-angular.umd.js",
  "module": "./dist/ax-angular.esm.js",
  "types": "./index.d.ts",
  "files": [
    "dist",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:meta && npm run build:js",
    "build:js": "rimraf dist/* && rollup -c",
    "build:meta": "node types.config.js",
    "examples": "npm run build && vite build",
    "prepack": "npm run build && rimraf out/*",
    "preview": "npm run examples && vite preview --open /",
    "start": "vite --open /",
    "test": "npm run examples && jest"
  },
  "dependencies": {
    "@activewidgets/datagrid": "3.1.2",
    "@activewidgets/frameworks": "3.1.2"
  },
  "peerDependencies": {
    "@angular/core": "*"
  },
  "devDependencies": {
    "@activewidgets/examples": "0.0.15",
    "@activewidgets/options": "3.1.2",
    "@activewidgets/puppeteer": "0.0.6",
    "@angular/common": "^13",
    "@angular/compiler": "^13",
    "@angular/core": "^13",
    "@angular/platform-browser": "^13",
    "@angular/platform-browser-dynamic": "^13",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@rollup/plugin-babel": "^5",
    "@rollup/plugin-node-resolve": "^13",
    "@testing-library/dom": "^8",
    "express": "^4",
    "ng-dynamic-component": "^10",
    "rimraf": "^3",
    "rollup": "^2",
    "rollup-plugin-md": "^1",
    "rollup-plugin-postcss": "^4",
    "rollup-plugin-sourcemaps": "^0.6",
    "rollup-plugin-terser": "^7",
    "rxjs": "~7.5.0",
    "vite": "^2",
    "zone.js": "~0.11.4"
  },
  "jest": {
    "preset": "@activewidgets/puppeteer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "activewidgets",
    "datagrid",
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://activewidgets.com"
}
