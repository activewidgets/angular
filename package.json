{
  "name": "@activewidgets/angular",
  "version": "0.0.9",
  "description": "Datagrid component for angular",
  "author": "ActiveWidgets SARL",
  "license": "MIT",
  "main": "./dist/ax-angular.umd.js",
  "module": "./dist/ax-angular.esm.js",
  "typings": "./index.d.ts",
  "files": [
    "css",
    "dist",
    "*.d.ts",
    "index.*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:source && npm run build:aot && rimraf tmp ",
    "build:js": "rimraf dist/* && rollup -c",
    "build:source": "node config/source.js",
    "build:aot": "ngc -p config/aot.json",
    "public": "npm run build && rimraf public/* && parcel build examples/index.html -d public",
    "start": "parcel examples/index.html -d public --open",
    "test": "npm run public && jest"
  },
  "dependencies": {
    "@activewidgets/frameworks": "0.0.9",
    "@activewidgets/datagrid": "0.0.9",
    "@angular/core": "*"
  },
  "devDependencies": {
    "@activewidgets/options": "0.0.9",
    "@activewidgets/puppeteer": "0.0.3",
    "@activewidgets/testing": "0.0.2",
    "@angular/common": "^8",
    "@angular/compiler": "^8",
    "@angular/compiler-cli": "^8",
    "@angular/platform-browser": "^8",
    "@angular/platform-browser-dynamic": "^8",
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "ng-dynamic-component": "^5",
    "parcel": "^1",
    "rimraf": "^3",
    "rollup": "^1.20",
    "rollup-plugin-babel": "^4",
    "rollup-plugin-node-resolve": "^4",
    "rxjs": "^6",
    "typescript": ">=3.4 <3.6",
    "zone.js": "~0.9.1"
  },
  "jest": {
    "projects": [
      {
        "displayName": "Unit",
        "moduleNameMapper": {
          "@activewidgets/components": "<rootDir>/",
          "@activewidgets/testing": "<rootDir>/test/adapter/angular.js"
        },
        "preset": "@activewidgets/testing",
        "setupFilesAfterEnv": [
          "<rootDir>/test/adapter/setup.js"
        ]
      },
      {
        "displayName": "Visual",
        "preset": "@activewidgets/puppeteer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://github.com/activewidgets/angular#readme"
}
