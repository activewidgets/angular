{
  "name": "@activewidgets/angular",
  "version": "0.0.6",
  "description": "Datagrid component for angular",
  "author": "webmaster@activewidgets.com",
  "license": "MIT",
  "main": "./dist/ax-angular.umd.js",
  "module": "./dist/ax-angular.esm.js",
  "typings": "./index.d.ts",
  "files": [
    "css",
    "dist",
    "*.d.ts",
    "index.*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:source && npm run build:aot && rimraf tmp ",
    "build:source": "node config/source.js",
    "build:js": "rollup -c",
    "build:aot": "ngc -p config/aot.json",
    "public": "parcel build examples/index.html -d public --no-minify",
    "start": "parcel examples/index.html -d public --open",
    "test": "npm run build && npm run public && jest"
  },
  "dependencies": {
    "@activewidgets/frameworks": "https://cdn.activewidgets.com/packages/frameworks-0.0.6.tgz",
    "@activewidgets/datagrid": "https://cdn.activewidgets.com/packages/datagrid-0.0.6.tgz",
    "@angular/core": "*"
  },
  "devDependencies": {
    "@activewidgets/puppeteer": "^0.0.3",
    "@activewidgets/testing": "^0.0.1",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "ng-dynamic-component": "^5.0.0",
    "parcel": "^1.0.0",
    "rimraf": "^2.0.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rxjs": "^6.0.0",
    "typescript": ">=3.4 <3.6",
    "zone.js": "~0.9.1"
  },
  "jest": {
    "projects": [
      {
        "displayName": "Unit",
        "moduleNameMapper": {
          "@activewidgets/components": "<rootDir>/",
          "@activewidgets/testing": "<rootDir>/test/adapter/angular.js"
        },
        "preset": "@activewidgets/testing",
        "setupFilesAfterEnv": [
          "<rootDir>/test/adapter/setup.js"
        ]
      },
      {
        "displayName": "Visual",
        "preset": "@activewidgets/puppeteer"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://github.com/activewidgets/angular#readme"
}
