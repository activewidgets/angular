{
  "name": "@activewidgets/angular",
  "version": "0.0.5",
  "description": "Datagrid component for angular",
  "author": "webmaster@activewidgets.com",
  "license": "MIT",
  "main": "./dist/ax-angular.umd.js",
  "module": "./dist/ax-angular.esm.js",
  "typings": "./index.d.ts",
  "files": [
    "css",
    "dist",
    "*.d.ts",
    "index.*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "e2e": "npm run storybook:build && npm run e2e:local",
    "e2e:local": "wdio ./test/config/wdio.local.js",
    "build": "npm run build:js && npm run build:source && npm run build:aot && rimraf tmp ",
    "build:source": "node config/source.js",
    "build:js": "rollup -c",
    "build:aot": "ngc -p config/aot.json",
    "storybook": "npm run build && start-storybook -p 9001 -c storybook",
    "storybook:build": "npm run build && rimraf out/storybook && build-storybook -c storybook -o out/storybook",
    "test": "jest"
  },
  "dependencies": {
    "@activewidgets/frameworks": "https://cdn.activewidgets.com/packages/frameworks-0.0.5.tgz",
    "@activewidgets/grid": "https://cdn.activewidgets.com/packages/grid-0.0.5.tgz"
  },
  "devDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular-devkit/core": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@storybook/angular": "^5.0.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.0.0",
    "@wdio/cli": "^5.0.0",
    "@wdio/jasmine-framework": "^5.0.0",
    "@wdio/local-runner": "^5.0.0",
    "@wdio/spec-reporter": "^5.0.0",
    "acorn": "^6.0.0",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.0",
    "chromedriver": "^2.45.0",
    "core-js": "^2.0.0",
    "express": "^4.0.0",
    "jest": "^24.0.0",
    "jest-preset-angular": "^7.0.0",
    "rimraf": "^2.0.0",
    "rollup": "^1.0.0",
    "rollup-plugin-babel": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rxjs": "^6.0.0",
    "ts-node": "^8.0.0",
    "tslint": "^5.0.0",
    "typescript": "~3.2.0",
    "wdio-chromedriver-service": "^5.0.0",
    "webdriverio": "^5.0.0",
    "zone.js": "^0.8.26"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/test/unit"
    ],
    "testMatch": [
      "**/*.ts"
    ],
    "testRegex": "",
    "setupFilesAfterEnv": [
      "<rootDir>/test/config/jest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://github.com/activewidgets/angular#readme"
}
