{
  "name": "@activewidgets/angular",
  "version": "0.0.1",
  "description": "Datagrid component for angular",
  "author": "webmaster@activewidgets.com",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "metadata": "dist/index.metadata.json",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "node scripts/aot.js && ngc -p scripts/aot.json && node scripts/inline.js && rimraf dist/index.ts",
    "storybook": "npm run build && start-storybook -p 9001 -c storybook",
    "storybook:build": "npm run build && rimraf out/storybook && build-storybook -c storybook -o out/storybook",
    "docs": "gitbook serve docs out/docs",
    "docs:build": "rimraf out/docs && gitbook build docs out/docs",
    "test": "jest"
  },
  "dependencies": {
    "@activewidgets/frameworks": "https://cdn.activewidgets.com/packages/frameworks-0.0.1.tgz",
    "@activewidgets/grid": "https://cdn.activewidgets.com/packages/grid-0.0.1.tgz"
  },
  "devDependencies": {
    "@angular/common": "^5.2.5",
    "@angular/compiler": "^5.2.5",
    "@angular/compiler-cli": "^5.2.5",
    "@angular/core": "^5.2.5",
    "@angular/forms": "^5.2.5",
    "@angular/platform-browser": "^5.2.5",
    "@angular/platform-browser-dynamic": "^5.2.5",
    "@storybook/addon-actions": "^3.4.0-alpha.8",
    "@storybook/addon-links": "^3.4.0-alpha.8",
    "@storybook/addon-notes": "^3.4.0-alpha.8",
    "@storybook/addons": "^3.4.0-alpha.8",
    "@storybook/angular": "^3.4.0-alpha.8",
    "@types/jest": "^22.1.2",
    "@types/node": "~9.4.6",
    "babel-core": "^6.26.0",
    "core-js": "^2.4.1",
    "gitbook-cli": "^2.3.2",
    "jest": "^22.3.0",
    "jest-preset-angular": "^5.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.6",
    "ts-node": "~4.1.0",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.20"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "testMatch": ["**/test/unit/**/*.ts"],
    "testRegex": "",
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup-jest.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/activewidgets/angular.git"
  },
  "keywords": [
    "angular"
  ],
  "bugs": {
    "url": "https://github.com/activewidgets/angular/issues"
  },
  "homepage": "https://github.com/activewidgets/angular#readme"
}
